<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" maxWidth="Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.editor.controllers.ApplicationPropertyController">
   <children>
      <Label fx:id="appTitleLabel" text="Google Chrome">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <ImageView fx:id="appIconImageView" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </ImageView>
      <Label maxWidth="1.7976931348623157E308" text="Path">
         <VBox.margin>
            <Insets left="10.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="pathTextField" editable="false">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </VBox.margin>
      </TextField>
      <Button fx:id="changeAppBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Change Application...">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets top="10.0" />
   </padding>
</VBox>
