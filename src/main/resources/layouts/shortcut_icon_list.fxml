<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.paint.*?>

<BorderPane prefHeight="500" prefWidth="300" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.control_panel.controllers.ShortcutIconListController">
  <top>
    <TextField fx:id="searchTextField" promptText="%search_icon" />
  </top>
  <bottom>
    <ToolBar>
      <items>
        <!--<Button fx:id="addExternalAppBtn" mnemonicParsing="false" text="Add External App..." textAlignment="CENTER" />-->
        <Pane HBox.hgrow="ALWAYS" />
        <Button fx:id="cancelBtn" alignment="CENTER" text="%cancel" textAlignment="CENTER" />
        <Button fx:id="selectBtn" alignment="CENTER" text="%select" textAlignment="CENTER" />
      </items>
    </ToolBar>
  </bottom>
  <center>
    <ListView fx:id="shortcutListView" prefHeight="200.0" prefWidth="200.0" />
  </center>
</BorderPane>
